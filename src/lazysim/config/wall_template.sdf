<?xml version="1.0"?>
<sdf version="1.6">
    <model name="wall">
        <static>{static}</static>
        <link name="link">
            <inertial>
                <mass>0.2</mass>
                <inertia>
                    <ixx>0.01</ixx>
                    <iyy>0.01</iyy>
                    <izz>0.01</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            <visual name="visual">
                <geometry>
                    <box>
                        <size>{sz_x} {sz_y} 0.1</size>
                    </box>
                </geometry>
                <material>
                    <script>
                        <uri>file://media/materials/scripts/gazebo.material</uri>
                        <name>Gazebo/{color}</name>
                    </script>
                </material>
            </visual>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>{sz_x} {sz_y} 0.1</size>
                    </box>
                </geometry>
            </collision>
        </link>
    </model>
</sdf>